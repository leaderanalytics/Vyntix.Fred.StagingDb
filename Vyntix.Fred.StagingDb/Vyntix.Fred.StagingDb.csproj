	  <RepositoryUrl>https://github.com/leaderanalytics/Vyntix.Fred.StagingDb.git</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>Leader Analytics LeaderAnalytics Vyntix FRED ALFRED vintage client FredClient economic economic-data economicdata</PackageTags>
	  <PackageReleaseNotes>See Repository</PackageReleaseNotes>
	  <PackageId>LeaderAnalytics.Vyntix.Fred.StagingDb</PackageId>
	  <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>
	
	<PropertyGroup>
		<RootNamespace>LeaderAnalytics.Vyntix.Fred.StagingDb</RootNamespace>
		<AssemblyName>LeaderAnalytics.Vyntix.Fred.StagingDb</AssemblyName>
